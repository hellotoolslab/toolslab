{
  "version": 2,
  "alias": ["toolslab.dev", "www.toolslab.dev"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "X-Corporate-VPN-Compatible",
          "value": "true"
        },
        {
          "key": "X-HSTS-Policy",
          "value": "disabled"
        }
      ]
    }
  ],
  "regions": ["iad1"],
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/tools",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/json",
      "destination": "/tools/json-formatter",
      "permanent": true
    },
    {
      "source": "/jwt",
      "destination": "/tools/jwt-decoder",
      "permanent": true
    },
    {
      "source": "/base64",
      "destination": "/tools/base64",
      "permanent": true
    }
  ],
  "crons": []
}
