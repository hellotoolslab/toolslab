# Production Environment Configuration Example
# Copy this file to .env.production for production deployment

# Vercel Edge Config (Production)
EDGE_CONFIG="your-production-edge-config-url"
EDGE_CONFIG_ID=your-production-edge-config-id

# Admin Access
ADMIN_SECRET_KEY=your-secure-admin-key
VERCEL_API_TOKEN=your-vercel-api-token

# Environment
VERCEL_ENV=production
NODE_ENV=production

# Analytics (Production)
NEXT_PUBLIC_UMAMI_WEBSITE_ID=your-umami-website-id
NEXT_PUBLIC_UMAMI_SCRIPT_URL=https://cloud.umami.is/script.js
NEXT_PUBLIC_UMAMI_HOST_URL=https://cloud.umami.is

# Production Feature Flags
NEXT_PUBLIC_ENABLE_ADS=true
NEXT_PUBLIC_ENABLE_PRO=true
NEXT_PUBLIC_MAINTENANCE_MODE=false

# IndexNow API Key for SEO auto-submission
INDEXNOW_API_KEY=your-indexnow-api-key

# VPN Support Configuration (Production)
NEXT_PUBLIC_VPN_MODE_ENABLED=true
NEXT_PUBLIC_ALLOW_INSECURE_CONNECTIONS=true
NEXT_PUBLIC_VPN_DEBUG_MODE=false