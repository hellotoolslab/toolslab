# Vercel Edge Config
EDGE_CONFIG=

# Environment
VERCEL_ENV=development
NODE_ENV=development

# Admin Access
ADMIN_SECRET_KEY=your-super-secret-admin-key-here

# Analytics
NEXT_PUBLIC_UMAMI_WEBSITE_ID=6e294d5a-5c43-4720-8753-6aa3ab169f4b
NEXT_PUBLIC_UMAMI_SCRIPT_URL=https://cloud.umami.is/script.js
NEXT_PUBLIC_UMAMI_HOST_URL=https://cloud.umami.is

# Feature Flags (can be overridden by Edge Config)
NEXT_PUBLIC_ENABLE_ADS=false
NEXT_PUBLIC_ENABLE_PRO=true
NEXT_PUBLIC_MAINTENANCE_MODE=false

# API Configuration
API_BASE_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Security
NEXTAUTH_SECRET=your-nextauth-secret-here
NEXTAUTH_URL=http://localhost:3000

# Development Tools
NEXT_PUBLIC_DEBUG_MODE=true
ANALYZE=false